<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="me.eoh_digital.model.Account" table="ACCOUNT">
        <meta attribute="class-description">
            This class contains the account details.
        </meta>
        <id name="accountId" type="int" column="accountId">
            <generator class="native"/>
        </id>
        <!--<property name="accountId" column="accountId" type="int"/>-->
        <property name="accountNumber" column="accountNumber" type="string"/>
    </class>
</hibernate-mapping>